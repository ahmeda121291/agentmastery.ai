{
  "name": "agentmastery-ai",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.13.1",
  "scripts": {
    "preinstall": "corepack enable",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css}\"",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "generate:answers": "tsx scripts/generate_answers.ts",
    "answers:seed:17": "tsx scripts/generate_answers.ts --count=17",
    "generate:posts": "tsx scripts/generate_posts.ts",
    "generate:bulk": "tsx scripts/bulk_generate.ts",
    "fix:mdx": "tsx scripts/fix_mdx_links.ts",
    "migrate:blog": "tsx scripts/migrate_blog_dir.ts"
  },
  "dependencies": {
    "@mdx-js/loader": "^3.1.1",
    "@mdx-js/react": "^3.1.1",
    "@next/mdx": "^15.5.3",
    "@radix-ui/react-popover": "^1.0.7",
    "@types/canvas-confetti": "^1.9.0",
    "canvas-confetti": "^1.9.3",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.2",
    "framer-motion": "^12.23.18",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.460.0",
    "next": "14.2.5",
    "next-mdx-remote": "^5.0.0",
    "openai": "^5.22.0",
    "react": "^18",
    "react-dom": "^18",
    "reading-time": "^1.5.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "remark-toc": "^9.0.0",
    "tailwind-merge": "^2.3.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/node": "^20.19.17",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "eslint-config-prettier": "^10.1.8",
    "postcss": "^8",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
